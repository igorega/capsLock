section.subscribe
	.container
		.subscribe__box
			i.subscribe__box-bg-icon
				+boxBg1

			i.subscribe__box-bg-icon
				+boxBg2

			.subscribe__box-bg
				img(src='img/components/subscribe/img/bg.svg', alt='')

			.subscribe__box-icon
				img(src='img/components/subscribe/img/icon.svg', alt='')

			h2.subscribe__title Subscribe to get information, latest news and other interesting offers about Cobham

			form.subscribe__form
				.subscribe__form-group
					i.subscribe__form-icon: +mail

					input#emailSubscribe.subscribe__form-input(
						type='email'
						name='emailSubscribe'
						title='Your email'
						placeholder='Your email'
						required
					)

				button.btn-subscribe(type='submit') Subscribe
